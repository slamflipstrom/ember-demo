{{input
  type="text"
    id="new-contact"
    placeholder="NEW CONTACT!"
    value=newFirstname
    action="createContact"
}}

<ul>
{{#each contact in model}}
<h3 id="title">{{contact.firstname}}  {{contact.lastname}}</h3>
  <li>{{contact.address}}</li>
  <li>{{contact.country}}</li>
  <li>{{contact.city}}</li>
  <li>{{contact.state}}</li>
  <li>{{contact.zip}}</li>
  <li>{{contact.phone}}</li>
  <li>{{contact.email}}</li>
{{/each}}
</ul>
